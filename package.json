{
  "name": "blushproof",
  "fullName": "Blushproof",
  "id": "blushproof@mozilla.org",
  "description": "Stop opening embarrassing sites accidentally",
  "author": "David Keeler, Monica Chew, Gregg Lind",
  "license": "MPL 2.0",
  "version": "0.9",
  "dependencies": [
    "addon-sdk",
    "micropilot"
  ],
  "preferences": [
    {
      "type": "bool",
      "title": "Enable debug messages",
      "name": "micropilotlog",
      "value": true
    },
    {
      "type": "bool",
      "title": "Report metrics",
      "name": "enable_reporting",
      "value": true
    }
  ],
  "volo": {
    "dependencies": {
      "packages/micropilot": "github:gregglind/micropilot/v0.10.0"
    }
  }
}
